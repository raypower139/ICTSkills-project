<table class="ui table">
    <thead>
        <tr>
          <th>Weight</th>
          <th>Chest</th>
          <th>Thigh</th>
          <th>Upper Arm</th>
          <th>Waist</th>
          <th>Hips</th>
          <th>Comment</th>
        </tr>
      </thead>
  <tbody>
    {{#each member.assessments}}
      <tr>
            <td>
              {{weight}}
            </td>
            <td>
              {{chest}}
            </td>
            <td>
              {{thigh}}
            </td>
            <td>
              {{upperarm}}
            </td>
            <td>
              {{waist}}
            </td>
            <td>
              {{hips}}
            </td>
            <td>
              {{comment}}
              
            </td>
            <td>
            <a href="/member/{{../member.id}}/deleteassessment/{{id}}" class="ui tiny red button">Delete Assessment</a>
            </td>
          </tr>
    {{/each}}
  </tbody>
</table>